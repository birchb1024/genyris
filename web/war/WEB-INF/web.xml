<?xml version="1.0" encoding="ISO-8859-1"?>
  <!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Genyris</display-name>
    <servlet>
        <servlet-name>GenyrisServlet</servlet-name>
        <servlet-class>org.genyris.web.GenyrisServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GenyrisServlet</servlet-name>
        <url-pattern>/genyris/*</url-pattern>
    </servlet-mapping>

</web-app>
