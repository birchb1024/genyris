@prefix rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
@prefix rdfs "http://www.w3.org/2000/01/rdf-schema#"
'rdf.Resource
  var !rdfs.isDefinedBy "http://www.w3.org/2000/01/rdf-schema#"
  var !rdfs.label "Resource"
  var !rdfs.comment "The class resource, everything."

'rdf.class
   var !rdfs.comment "The class of classes."
   var !rdfs.subClassof 'rdfs.Resource