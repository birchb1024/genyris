   or
      equal? i a
      equal? j b
      equal? (- i j) (- a b)
      equal? (+ i j) (+ a b)
      
include "examples/queens.lin"
queens 8

# mark the number 24 as a member of class 'C'
tag C 24     # => ~ 24 ; Bignum C

define pitt
  dict 
    .name  = "Willam Pitt"
    .title = "Prime Minister"
    .date-of-birth   = "28 May 1759"

assert 
    equal? (pitt.name) "Willam Pitt"
    
pitt
   .name = "William Pitt The Younger"
pitt 
   define .father "William Pitt the Elder"
assert
    equal? 
        pitt.vars
        ^(father title name date-of-birth vars)

