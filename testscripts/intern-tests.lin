load "testscripts/gunit.lin"
@prefix t "http//www.genyris.org/lib/gunit#"
@prefix u "http://prefix.genyris.org/lang/utilities#"

;; Test built-in interning 
@prefix p "P"

var overall-result
  t.test "Intern tests"
    t.given '(intern "A")       expect 'A
    t.given '(intern "Pw")       expect 'p.w
    t.given (left (member? Symbol ((intern 23)!classes)))       expect Symbol
    t.given '(intern "Pw")       expect 'p.w

or overall-result (raise "Maths Suite Failed")

