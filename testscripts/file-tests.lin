def dump()
  var fi (File(_new "C:\\foo.txt"))
  var in (fi (_open 'read))
  var ch nil
  var count 0
  while (in(_hasData))
     set! count (+ count 1)
     print (in(_read)) '-
  in(_close)
  print "\n" count
   

def parse-file()
  var fi (File(_new "C:\\foo.lin"))
  var in (fi (_open 'read))
  var parser (Parser(_new in))
  var exp nil
  while
     not
        equal? EOF
           set! exp (parser(_read))
     print exp
     display "\n"
  parser(_close)
   

def parse-sfs-file()
  var fi (File(_new "C:\\foo.sfs"))
  var in (fi (_open 'read))
  var sfs (StringFormatStream(_new in))
  var parser (Parser(_new sfs))
  var exp nil
  while
     not
        equal? EOF
           set! exp parser(_read)
     write exp
     display "\n"
  parser(_close)
   