;; File Handling Module (using prefixes)

@prefix file "genyris:/files/"


def !file.copy(from to) ...
def !file.delete(filename) ...
def !file.zip(file) ...


;; Use of the file module
@prefix f "genyris:/files/"

def archive(filename)
    f.copy filename "/tmp/foo"
    f.zip "/tmp/foo"
    f.delete filename